{
  "name": "vibe-gain",
  "version": "0.0.14",
  "description": "Visualize GitHub commit patterns to evaluate vibe-coding productivity gains",
  "type": "module",
  "main": "./lib/index.js",
  "browser": "./browser/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./browser": "./browser/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyberchitta/vibe-gain.git"
  },
  "scripts": {
    "start": "bun lib/index.js",
    "start:custom": "bun lib/index.js ./custom-parameters.json",
    "serve": "bunx http-server . --cors -p 3001",
    "gen-llm-report": "node lib/export/gen-all-plots-llm.js"
  },
  "dependencies": {
    "@octokit/graphql": "^8.2.2",
    "@octokit/rest": "^21.1.1",
    "dotenv": "^16.6.1"
  },
  "optionalDependencies": {
    "vega": "^6.1.2",
    "vega-lite": "^6.2.0"
  },
  "license": "Apache-2.0"
}
